def neither_divisible_by_5nor2(n):
    if n%5!=0 and n%2!=0:
        print(f"{n} is neither divisible by 5 nor 2")
    elif n%5==0:
        print(f"{n} is divisible by 5")
    elif n%2==0:
        print(f"{n} is divisible by 2")
 
num=int(input("Enetr input:"))
neither_divisible_by_5nor2(num)